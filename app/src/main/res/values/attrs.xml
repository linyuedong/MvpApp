<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 加载状态布局 -->
    <declare-styleable name="LoadingLayout">
        <!--加载中布局-->
        <attr name="loading_layout" format="reference" />
        <attr name="loading_img" format="reference" />
        <attr name="loading_tv" format="string" />
        <!--空布局-->
        <attr name="empty_layout" format="reference" />
        <attr name="empty_img" format="reference" />
        <attr name="empty_tv" format="string" />
        <!--错误布局-->
        <attr name="error_layout" format="reference" />
        <attr name="error_img" format="reference" />
        <attr name="ldl_error_tv" format="string" />
        <!--无网络布局-->
        <attr name="net_layout" format="reference" />
        <attr name="net_img" format="reference" />
        <attr name="net_tv" format="string" />
    </declare-styleable>
</resources>
